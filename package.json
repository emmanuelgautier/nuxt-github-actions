  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/pwa": "3.3.5",
    "core-js": "3.19.2",
    "nuxt": "2.15.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.3",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@cypress/vue": "2.2.3",
    "@cypress/webpack-dev-server": "1.7.0",
    "@emmanuelgautier/renovate-config": "1.2.2",
    "@lhci/cli": "0.8.2",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "7.0.2",
