    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "28.1.2",
    "cypress": "10.3.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-vue": "9.2.0",
