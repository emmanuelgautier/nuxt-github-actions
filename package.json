    "babel-jest": "27.4.2",
    "cypress": "9.1.1",
    "eslint": "8.4.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-nuxt": "3.1.0",
    "eslint-plugin-vue": "8.1.1",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "jest": "27.4.3",
    "lint-staged": "12.1.2",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.1.0",
    "stylelint-config-html": "1.0.0",
